-on:click=\"reloadTodo\" style=\"display: block; margin: auto\">Reload</v-btn>\n          </v-col>\n        </v-row>\n      <v-data-table :headers=\"table.headers\" :items=\"todos\">\n    </v-data-table>\n    </div>\n              </v-card>\n  </v-container>\n</template>\n\n<script lang=\"ts\">\nimport Vue from \"vue\";\nimport {Todo} from \"@/types/todo\";\nimport {createTodo, getTodos, clearTodos} from \"@/api/own\";\n\nexport default Vue.extend({\n  name: \"Todo\",\n  data: function () {\n    return {\n      isDark: true,\n      show: true,\n      newTodo: \"\",\n      todos: Array<Todo>(),\n      table: {\n        headers: [\n          {\n            text: \"Complete?\",\n            value: \"isDone\"\n          },\n          {\n            text: \"Title\",\n            value: \"title\"\n          },\n        ]\n      }\n    };\n  },\n  methods: {\n    async addTodo() {\n      const value = this.newTodo && this.newTodo.trim();\n      if (!value) {\n        return;\n      }\n\n      await createTodo(this.newTodo);\n\n      this.newTodo = \"\";\n\n      this.reloadTodo();\n    },\n\n    async clearTodo() {\n      await clearTodos();\n\n      this.reloadTodo();\n    },\n\n    reloadTodo() {\n      getTodos().then(todos => {\n        this.$data.todos = todos;\n      });\n    }\n  }  ,\n  mounted() {\n    this.reloadTodo();\n  }\n});\n\n</script>\n"]}]}